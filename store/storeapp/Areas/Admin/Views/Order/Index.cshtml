@model IEnumerable<Order>

@if (Model.Count() > 0)
{
    <table class="table table-hover">
        @foreach (Order order in Model)
        {
            <div class="my-3">
                <partial name="_OrderDetails" model="@order" /> 
            </div>
        }
    </table>
}
else
{
    <div class="alert alert-info">
        <p class="lead">
            There is no order yet.
        </p>
    </div>
}